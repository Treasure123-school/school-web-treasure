modules = ["nodejs-20", "web"]
[agent]
expertMode = true
integrations = ["javascript_supabase:1.0.0", "javascript_database:1.0.0"]

[nix]
channel = "stable-25_05"
packages = ["sqlite"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Start application"

[[workflows.workflow]]
name = "Start application"
author = "agent"

[workflows.workflow.metadata]
outputType = "webview"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npm run dev"
waitForPort = 5000

[[workflows.workflow]]
name = "Verify Supabase Storage"
author = 48707310
mode = "sequential"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npx tsx verify-supabase-storage.ts"

[deployment]
deploymentTarget = "autoscale"
build = ["npm", "run", "build"]
run = ["npm", "run", "start"]

[userenv]

[userenv.shared]
MINIO_USE_SSL = "false"
STORAGE_MODE = "disk"
UPLOAD_DIR = "uploads"

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 35647
externalPort = 3002

[[ports]]
localPort = 42611
externalPort = 3000

[[ports]]
localPort = 43859
externalPort = 3001
